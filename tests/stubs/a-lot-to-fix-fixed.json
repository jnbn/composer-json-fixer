{
    "keywords": [
        "keyword A",
        "keyword B"
    ],
    "license": "proprietary",
    "require": {
        "php": "^5.5 || ^7.0",
        "roave/security-advisories": "dev-master",
        "vendor/package-1": ">1.0",
        "vendor/package-2": ">=1.0",
        "vendor/package-3": "<1.0",
        "vendor/package-4": "<=1.0",
        "vendor/package-5": "!=1.0",
        "vendor/package-6": "1.0, 2.0",
        "vendor/package-7": "1.0 || 2.0",
        "vendor/package-8": "1.0 - 2.0",
        "vendor/package-9": ">=1.0 <2.0",
        "vendor/package-10": "1.0",
        "zorro": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Monolog\\": "src/",
            "Vendor_Namespace\\": "src/"
        },
        "psr-4": {
            "": "/",
            "Monolog\\": "src/",
            "Vendor\\Namespace\\": "/"
        },
        "classmap": [
            "ClassOne.php",
            "ClassTwo.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "www.example.com/repo1"
        },
        {
            "type": "vcs",
            "url": "www.example.com/repo2"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
