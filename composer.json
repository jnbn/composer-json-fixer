{
    "name": "kubawerlos/composer-json-fixer",
    "description": "A tool to fix composer.json file",
    "type": "library",
    "keywords": [
        "composer",
        "fixer"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kuba WerÅ‚os",
            "email": "werlos@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.6",
        "ext-json": "*",
        "sebastian/diff": "^1",
        "symfony/console": "^2.7 || ^3",
        "symfony/filesystem": "^2.7 || ^3",
        "symfony/finder": "^2.7 || ^3",
        "symfony/process": "^2.7 || ^3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.3",
        "phpunit/phpunit": "^5.7",
        "squizlabs/PHP_CodeSniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "ComposerJsonFixer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "composer-json-fixer"
    ]
}
